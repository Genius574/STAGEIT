<h1>Items#create</h1>
<p>Find me in app/views/items/create.html.erb</p>
<%= simple_form_for(@item) do |f| %>
  <%= f.input :name %>
  <%= f.input :category, collection: [["Event Setup", "event_set_up"], ["Sound System", "sound_system"]], prompt: "Select a category" %>  <%= f.input :description %>
  <%= f.input :price %>
  <%= f.submit %>
<% end %>
<%= link_to "back to items", items_path %>
